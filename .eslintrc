{
  "extends": [
    "react-app",
    "prettier"
  ],
  "rules": {
    "promise/always-return": "error",
    "default-case": "error",
    "eqeqeq": "error",
    "object-curly-spacing": ["error", "always"],
    "no-var": "error",
    "react/jsx-curly-spacing": [2, { "when": "always", "children": true }],
    "jsx-quotes": [
      1,
      "prefer-double"
    ]
  },
  "plugins": [
    "prettier",
    "promise"
  ]
}